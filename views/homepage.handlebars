<!-- Loop over each object in the projects array -->
{{#each blog as |blog| }}
<div class="row mb-4 blog">
  <div class="col-md-5">
    <h2>
      <a href="/blog/{{blog.id}}">{{blog.title}}</a>
    </h2>
  </div>
  <div class="col-md-7">
    <p>
      {{blog.contents}}
    </p>
  </div>
</div>
{{/each}}
